{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "wasp",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "definition"
      }
    },
    "definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "type"
        },
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "SYMBOL",
          "name": "value"
        }
      ]
    },
    "comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "//"
            },
            {
              "type": "PATTERN",
              "value": ".*"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "/*"
            },
            {
              "type": "PATTERN",
              "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
            },
            {
              "type": "STRING",
              "value": "*/"
            }
          ]
        }
      ]
    },
    "type": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "action"
          },
          {
            "type": "STRING",
            "value": "apiNamespace"
          },
          {
            "type": "STRING",
            "value": "api"
          },
          {
            "type": "STRING",
            "value": "app"
          },
          {
            "type": "STRING",
            "value": "entity"
          },
          {
            "type": "STRING",
            "value": "job"
          },
          {
            "type": "STRING",
            "value": "page"
          },
          {
            "type": "STRING",
            "value": "query"
          },
          {
            "type": "STRING",
            "value": "route"
          },
          {
            "type": "STRING",
            "value": "crud"
          }
        ]
      }
    },
    "variable": {
      "type": "PATTERN",
      "value": "[a-zA-Z][0-9a-zA-Z]*"
    },
    "value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "constant"
        },
        {
          "type": "SYMBOL",
          "name": "js_import"
        },
        {
          "type": "SYMBOL",
          "name": "prisma_closure"
        },
        {
          "type": "SYMBOL",
          "name": "json_closure"
        },
        {
          "type": "SYMBOL",
          "name": "array"
        },
        {
          "type": "SYMBOL",
          "name": "dictionary"
        }
      ]
    },
    "string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "string_content"
              },
              {
                "type": "SYMBOL",
                "name": "escape_sequence"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "string_content": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "[^\"\\\\]+"
        }
      }
    },
    "escape_sequence": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\\\."
      }
    },
    "number": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "-?\\d+(\\.\\d+)?"
      }
    },
    "constant": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        },
        {
          "type": "STRING",
          "value": "EmailAndPassword"
        },
        {
          "type": "STRING",
          "value": "PostgreSQL"
        },
        {
          "type": "STRING",
          "value": "SQLite"
        },
        {
          "type": "STRING",
          "value": "Simple"
        },
        {
          "type": "STRING",
          "value": "PgBoss"
        },
        {
          "type": "STRING",
          "value": "SMTP"
        },
        {
          "type": "STRING",
          "value": "SendGrid"
        },
        {
          "type": "STRING",
          "value": "Mailgun"
        },
        {
          "type": "STRING",
          "value": "Dummy"
        },
        {
          "type": "STRING",
          "value": "ALL"
        },
        {
          "type": "STRING",
          "value": "GET"
        },
        {
          "type": "STRING",
          "value": "POST"
        },
        {
          "type": "STRING",
          "value": "PUT"
        },
        {
          "type": "STRING",
          "value": "DELETE"
        }
      ]
    },
    "js_import": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "import"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "default_import"
            },
            {
              "type": "SYMBOL",
              "name": "named_import"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "default_import"
                },
                {
                  "type": "STRING",
                  "value": ","
                },
                {
                  "type": "SYMBOL",
                  "name": "named_import"
                }
              ]
            }
          ]
        },
        {
          "type": "STRING",
          "value": "from"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "default_import": {
      "type": "SYMBOL",
      "name": "variable"
    },
    "named_import": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "variable"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": "as"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "alias"
                      }
                    ]
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "STRING",
                    "value": ","
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "alias": {
      "type": "SYMBOL",
      "name": "variable"
    },
    "prisma_closure": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{=psl"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "[^=]*"
          }
        },
        {
          "type": "STRING",
          "value": "psl=}"
        }
      ]
    },
    "json_closure": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{=json"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "[^=]*"
          }
        },
        {
          "type": "STRING",
          "value": "json=}"
        }
      ]
    },
    "array": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "value"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "value"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "dictionary": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "dict_entry"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "dict_entry"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "dict_entry": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "value"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
