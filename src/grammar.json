{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "wasp",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "type_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "variable_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "value"
        }
      ]
    },
    "comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "//"
            },
            {
              "type": "PATTERN",
              "value": ".*"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "/*"
            },
            {
              "type": "PATTERN",
              "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
            },
            {
              "type": "STRING",
              "value": "*/"
            }
          ]
        }
      ]
    },
    "type_declaration": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 2,
        "content": {
          "type": "PATTERN",
          "value": "(action|apiNamespace|api|app|entity|job|page|query|route|crud)"
        }
      }
    },
    "variable_declaration": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "PATTERN",
        "value": "[a-zA-Z][0-9a-zA-Z]*"
      }
    },
    "value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "constant"
        },
        {
          "type": "SYMBOL",
          "name": "js_import"
        },
        {
          "type": "SYMBOL",
          "name": "prisma_closure"
        },
        {
          "type": "SYMBOL",
          "name": "json_closure"
        },
        {
          "type": "SYMBOL",
          "name": "array"
        },
        {
          "type": "SYMBOL",
          "name": "dict"
        },
        {
          "type": "SYMBOL",
          "name": "variable"
        }
      ]
    },
    "string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "string_content"
              },
              {
                "type": "PATTERN",
                "value": "[^\"\\\\]"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "number": {
      "type": "PATTERN",
      "value": "-?\\d+(\\.\\d+)?"
    },
    "constant": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "(true|false)"
        },
        {
          "type": "PATTERN",
          "value": "(EmailAndPassword|PostgreSQL|SQLite|Simple|PgBoss|SMTP|SendGrid|Mailgun|Dummy)"
        },
        {
          "type": "PATTERN",
          "value": "(ALL|GET|POST|PUT|DELETE)"
        }
      ]
    },
    "js_import": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "import"
        },
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "STRING",
          "value": "from"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "json_closure": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{=json"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_definition"
          }
        },
        {
          "type": "STRING",
          "value": "json=}"
        }
      ]
    },
    "prisma_closure": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{=psl"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_definition"
          }
        },
        {
          "type": "STRING",
          "value": "psl=}"
        }
      ]
    },
    "array": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "_definition"
                      },
                      {
                        "type": "STRING",
                        "value": ","
                      }
                    ]
                  }
                },
                {
                  "type": "SYMBOL",
                  "name": "_definition"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "dict": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "dict_key"
                      },
                      {
                        "type": "STRING",
                        "value": ":"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "_definition"
                      },
                      {
                        "type": "STRING",
                        "value": ","
                      }
                    ]
                  }
                },
                {
                  "type": "SYMBOL",
                  "name": "dict_key"
                },
                {
                  "type": "STRING",
                  "value": ":"
                },
                {
                  "type": "SYMBOL",
                  "name": "_definition"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "dict_key": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "[a-zA-Z_][0-9a-zA-Z_]*"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "variable": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "PATTERN",
        "value": "[a-zA-Z][0-9a-zA-Z]*"
      }
    },
    "string_content": {
      "type": "PATTERN",
      "value": "\\\\."
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [
    [
      "variable_declaration",
      "variable"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
